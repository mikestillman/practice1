# ifneq (@ETAGS@,)
# all: tags
# endif
# tags:: @srcdir@/TAGS
clean :: ; rm -f *.o *.dep *.s *.E LOGFILE
Makefile: Makefile.in; cd ../..; ./config.status Macaulay2/system/Makefile
distclean::; rm -f Makefile

