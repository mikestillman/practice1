# @configure_input@
include ../../../include/config.Makefile
VPATH = @srcdir@
DEPENDS = yes
include @srcdir@/../make.include

.PHONY : all place_into_lib

LINALGGB_FILES := MonomialOps Monomials MonomialSet MonomialTable SPairSet monoms interface 
# MonomialHeap 

LINALGGB_OBJECT_FILES :=  $(addsuffix .o, $(LINALGGB_FILES))

all :: $(LINALGGB_OBJECT_FILES) place_into_lib #check_it
place_into_lib : $(LINALGGB_OBJECT_FILES); $(AR) rcs ../$(LIBGB) $^
check_it : $(LINALGGB_OBJECT_FILES) check_it.o
	gcc -o check_it check_it.o $(LINALGGB_OBJECT_FILES) -L/sw/lib -lgc

%.s : %.c; $(COMPILE.cc) -S $< $(OUTPUT_OPTION)
